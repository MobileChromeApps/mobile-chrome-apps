{
  "name": "Hello World",
  "description": "A Sample Chrome App",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "0.1.1",
  "offline_enabled": true,
  "icons": {
    "16": "icons/icon16.png",
    "36": "icons/icon36.png",
    "48": "icons/icon48.png",
    "57": "icons/icon57.png",
    "72": "icons/icon72.png",
    "96": "icons/icon96.png",
    "114": "icons/icon114.png",
    "128": "icons/icon128.png",
    "144": "icons/icon144.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "<all_urls>",
    "http://developers.google.com/web/*"
  ]
}
